                function getZarr(str, Z) {
                    let n = str.length;


                    let L = 0,
                        R = 0;

                    for (let i = 1; i < n; ++i) {

                        if (i > R) {

                            L = R = i;


                            while (R < n && str[R - L] == str[R])
                                R++;

                            Z[i] = R - L;
                            R--;

                        } else {

                            let k = i - L;

                            if (Z[k] < R - i + 1)
                                Z[i] = Z[k];

                            else {


                                L = i;
                                while (R < n && str[R - L] == str[R])
                                    R++;

                                Z[i] = R - L;
                                R--;
                            }
                        }
                    }
                }

                //listareaction

                let rx = [

                    "https://i.ibb.co/Y2KGxmP/Babbo-reaction.jpg",
